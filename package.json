{
  "name": "skywebdriverio_test",
  "version": "1.0.0",
  "description": "Test Exercise to Automate Sky web page using WebdriverIO with Mocha",
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf ./reports",
    "test": "wdio wdio.conf.js",
    "test.sauce": "wdio wdio.saucelabs.conf.js",
    "lint": "eslint .",
    "report": "allure serve reports/allure-results",
    "report.ci": "allure generate reports/allure-results --clean -o allure-report",
    "report.teams": "node ./support/utils/teamsReporter.js ../../report/json/testResults.json"
  },
  "keywords": [
    "mocha",
    "uitesting",
    "webdriverio",
    "test automation",
    "javascript"
  ],
  "author": "DC <dudleycowan@yahoo.com>",
  "license": "ISC",
  "dependencies": {
    "allure-commandline": "^2.13.8",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "shx": "^0.3.2",
    "superagent": "^5.3.1",
    "wdio-video-reporter": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-plugin": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@wdio/allure-reporter": "^6.11.0",
    "@wdio/cli": "^6.12.0",
    "@wdio/junit-reporter": "^6.4.0",
    "@wdio/local-runner": "^6.12.0",
    "@wdio/mocha-framework": "^6.11.0",
    "@wdio/sauce-service": "^6.4.5",
    "@wdio/spec-reporter": "^6.11.0",
    "@wdio/sync": "^6.11.0",
    "chromedriver": "^87.0.5",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-mocha": "^6.3.0",
    "moment": "^2.27.0",
    "wdio-chromedriver-service": "^6.0.4",
    "wdio-json-reporter": "^2.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
